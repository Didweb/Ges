{% extends '::backend_login.html.twig' %}

{% block contenidos %}
<section id="contenidos_login">
<h1>Proceso de identificación</h1>
<p>
Acceso al panel de control de  {{ global.cliente_detalle }}.
</p>
{% if error %}
<div id="mensa_error">{{ error.message|trans() }}</div>
{% endif %}

<div  id="f_login">
		<form action="{{ path('login_check') }}" method="post" id="ellogin">
		 <div class="ca_log1">
		<label for="username" >Usuario:</label>
		 <input id="username" type="text" name="_username" value="{{ last_username }}" />
		 </div>
		 
		 <div class="ca_log1">
		 <label for="password" >Contraseña:</label>
		 <input id="password" type="password" name="_password"  />
		 </div>
		 <input type="hidden" name="_target_path" value="gestor_crud_homepage" />
		 <input type="submit"  name="login" id="boton_login" value="Acceso" title="Iniciar el proceso de identificación"   />
		</form>
</div>

</section>  

{% endblock %}
